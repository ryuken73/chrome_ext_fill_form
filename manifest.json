{
    "name": "helpit extension",
    "version": "1.0",
    "description": "fill html form using ajax response",
    "manifest_version": 2,
    "permissions" : ["declarativeContent"],
    "background" : {
        "scripts" : ["background.js"],
        "persistent" : false
    },
    "page_action" : {
        "default_popup" : "popup.html"
    },
    "content_scripts": [
        {
          "matches": ["https://wise.sbs.co.kr/*"],
          "run_at": "document_idle",
          "js": ["content.js"]
        }
    ]
  }